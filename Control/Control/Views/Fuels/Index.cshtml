@model IEnumerable<Control.Models.Fuel>
@{
    ViewData["Title"] = "Fuels Control";
}

<h2>@ViewData["Title"]</h2>


<p>
    <a asp-action="Create" class="btn btn-default">Create</a>
</p>

<table class="table table-condensed">
    <tbody>
        @foreach(var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(model => item.Name)
                </td>
                <td>
                    <a asp-action="Edit" asp-route-id="@item.Id"class="btn btn-default">Edit</a>
                    <a asp-action="Delete" asp-route-id="@item.Id" class="btn btn-default">Delete</a>
                </td>
            </tr>
        }
    </tbody>
</table>